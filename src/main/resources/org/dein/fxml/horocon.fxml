<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="500.0" styleClass="mainVbox"
      xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.dein.HoroconController">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="HOLOCRON DE LA FUERZA" />
      <TextField fx:id="txtNombre" prefHeight="26.0" prefWidth="170.0" promptText="Ingresa tu nombre, joven padawan..." />
      <Button fx:id="btnEnviar" mnemonicParsing="false" onAction="#enviarMensaje" text="Transmitir mensaje" />
      <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="btnRetirarse" mnemonicParsing="false" onAction="#salir" text="Retirarse" />
         </children>
      </HBox>
   </children>
</VBox>
